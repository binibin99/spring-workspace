import com.ezen.springrest.dto.EmployeeDTO;

@RequestMapping("/jquery")
@Controller
public class JQuerySampleController {

	@GetMapping("/")
	public String jQueryHome() {
		return "jquery/index";
	}
	
	@ResponseBody
	@GetMapping("/employee/{emp_id}")
	public EmployeeDTO getEmployee() {
		
	}
	
}
